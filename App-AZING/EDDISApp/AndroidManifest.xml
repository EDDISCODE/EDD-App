<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.eddisapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.eddisapp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.eddisapp.DisplaySettingsActivity"
            android:label="@string/title_activity_display_settings" >
        </activity>
        <activity
            android:name="com.example.eddisapp.UploadPhotoActivity"
            android:label="@string/title_activity_upload_photo" >
        </activity>
        <activity
            android:name="com.example.eddisapp.TakeActivity"
            android:label="@string/title_activity_take" >
        </activity>
    </application>

</manifest>
